/**
 * Rectangle class template
 */
class Rectangle {
    public String getName() {
        return name;
    }

    public int getLength() {
        return length;
    }

    public int getHeight() {
        return height;
    }

    private String name;
    private int length;
    private int height;

    Rectangle(String name, int length, int height){
        this.name = name;
        this.length = length;
        this.height = height;
    }

    @Override
    public String toString() {
        return "name is:"+ getName() + ", length="+ getLength() + ", height="+ getHeight();
    }
}
